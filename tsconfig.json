{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "moduleResolution": "node",
    "baseUrl": ".",
    "paths": {
      "assets": ["assets"],
      "components": ["src/components"],
      "services": ["src/services"],
      "screens": ["src/screens"],
      "res": ["src/res"]
    },
    "isolatedModules": true,
    "strict": true,
    "allowSyntheticDefaultImports": true,
    "skipLibCheck": true,
    "target": "esnext",
    "module": "commonjs",
    "lib": ["es2017"],
    "allowJs": true,
    "jsx": "react-native",
    "noEmit": true
  },
  "exclude": ["node_modules"]
}
